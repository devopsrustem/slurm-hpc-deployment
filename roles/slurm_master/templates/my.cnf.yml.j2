# MySQL client configuration for Slurm
# Generated by Ansible

[client]
user=root
password={{ mysql_root_password }}
host={{ database_host }}
port={{ database_port }}

[mysql]
user=root
password={{ mysql_root_password }}
host={{ database_host }}
port={{ database_port }}

[mysqldump]
user=root
password={{ mysql_root_password }}
host={{ database_host }}
port={{ database_port }}
single-transaction=true
routines=true
triggers=true